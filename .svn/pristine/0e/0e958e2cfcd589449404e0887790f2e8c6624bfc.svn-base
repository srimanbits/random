package com.ggk.hrms.review.ui.form;

import java.util.List;
import java.util.Map;

import com.ggk.hrms.review.ui.vo.DropDownVO;
import com.ggk.hrms.review.ui.vo.ReviewObjectiveVO;

public class ObjectiveForm {
	
	private List<ReviewObjectiveVO> objectives;
	private  Map<Integer, String>  projectDropDown;
	private  List<DropDownVO> defaultObjDropDown;


	

	

	public Map<Integer, String> getProjectDropDown() {
		return projectDropDown;
	}

	public void setProjectDropDown(Map<Integer, String> projectDropDown) {
		this.projectDropDown = projectDropDown;
	}

	public List<DropDownVO> getDefaultObjDropDown() {
		return defaultObjDropDown;
	}

	public void setDefaultObjDropDown(List<DropDownVO> defaultObjDropDown) {
		this.defaultObjDropDown = defaultObjDropDown;
	}

	public List<ReviewObjectiveVO> getObjectives() {
		return objectives;
	}
	
	public void setObjectives(List<ReviewObjectiveVO> objectives) {
		this.objectives = objectives;
	}

	

}
