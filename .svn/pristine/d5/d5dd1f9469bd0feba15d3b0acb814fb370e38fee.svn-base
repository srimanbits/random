package com.ggk.hrms.review.utils;

public enum AppraisalPeriodTypeEnum {
	ANNUAL_APPRAISAL("Annual", 1), HALF_YEARLY_APPRAISAL("Half-Yearly", 2);

	private String name;
	private Integer id;

	private AppraisalPeriodTypeEnum(String name, Integer id) {
		this.name = name;
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public Integer getId() {
		return id;
	}

}