package com.ggk.hrms.review.ui.vo;


public class AppraisalShareInfoVO {

	private Integer reviewHeaderId;
	private Boolean isShared;
	private Boolean isSharedByYou;
	private Boolean isSharedWithYou;
	private String sharedByEmployee;
	private String sharedWithEmployee;
	private Integer sharedWithEmployeeId;
	private Integer sharedByEmployeeId;
	public Integer getReviewHeaderId() {
		return reviewHeaderId;
	}
	public void setReviewHeaderId(Integer reviewHeaderId) {
		this.reviewHeaderId = reviewHeaderId;
	}
	
	
	public Boolean getIsShared() {
		return isShared;
	}
	public void setIsShared(Boolean isShared) {
		this.isShared = isShared;
	}
	public Boolean getIsSharedByYou() {
		return isSharedByYou;
	}
	public void setIsSharedByYou(Boolean isSharedByYou) {
		this.isSharedByYou = isSharedByYou;
	}
	public Boolean getIsSharedWithYou() {
		return isSharedWithYou;
	}
	public void setIsSharedWithYou(Boolean isSharedWithYou) {
		this.isSharedWithYou = isSharedWithYou;
	}
	public String getSharedByEmployee() {
		return sharedByEmployee;
	}
	public void setSharedByEmployee(String sharedByEmployee) {
		this.sharedByEmployee = sharedByEmployee;
	}
	public String getSharedWithEmployee() {
		return sharedWithEmployee;
	}
	public void setSharedWithEmployee(String sharedWithEmployee) {
		this.sharedWithEmployee = sharedWithEmployee;
	}
	public Integer getSharedWithEmployeeId() {
		return sharedWithEmployeeId;
	}
	public void setSharedWithEmployeeId(Integer sharedWithEmployeeId) {
		this.sharedWithEmployeeId = sharedWithEmployeeId;
	}
	public Integer getSharedByEmployeeId() {
		return sharedByEmployeeId;
	}
	public void setSharedByEmployeeId(Integer sharedByEmployeeId) {
		this.sharedByEmployeeId = sharedByEmployeeId;
	}
	
	
	
}
