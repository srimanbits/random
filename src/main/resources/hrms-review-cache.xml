<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">
    <!-- Rating,Competency,proficiencyLevel generally dont change frequently. Hence, cached for one day -->
    <cache name="ratingDropDown" maxElementsInMemory='15' timeToLiveSeconds="86400" timeToIdleSeconds="86400" overflowToDisk='false' />
    <cache name="proficiencyLevel" maxElementsInMemory="15" overflowToDisk="false" timeToLiveSeconds="86400" timeToIdleSeconds="86400"/>
    <cache name="project" maxElementsInMemory="100" overflowToDisk="false" timeToLiveSeconds="86400" timeToIdleSeconds="86400"/>
</ehcache>